//Programm mis ütleb kas kliendil on piisavalt raha.
// Iga korda kui käima paned tuleb uus klient uue rahakotiga.


rahakott = Math.random()*50 //$
tootehind = 25
if (rahakott < tootehind){
    console.log("Sul pole piisavalt raha, et seda osta")
}
else console.log("Sul on piisavalt raha, et seda osta")
console.log(rahakott - tootehind)
